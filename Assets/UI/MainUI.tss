.container {
    display: flex;
    justify-content: center; 
    align-items: center;
    height: 100%;
}

.centered-button {
    width: 20%;
    height: 100px;
    margin-left: auto;
    margin-right: auto;
    color: black;
    font-size: 20px;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
}